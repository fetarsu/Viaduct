<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Viaduct.Pages.ChooseUserPage"
             Title="ChooseUser">
    <ContentPage.Content>
        <StackLayout>
            <Button Text="Admin" Command="{Binding AdminCommand}"></Button>
            <Button Text="Manager" Command="{Binding ManagerCommand}"></Button>
            <Button Text="Deliverer" Command="{Binding DelivererCommand}"></Button>
            <Button Text="User" Command="{Binding UserCommand}"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>